{
  "name": "grabber",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dump_version": "node scripts/inject-version.js",
    "build_win32_x64": "npx electron-packager . --platform=win32 --arch=x64 --ignore=\"^(/build|/scripts)\" --out build",
    "build_win32_arm64": "npx electron-packager . --platform=win32 --arch=arm64 --ignore=\"^(/build|/scripts)\" --out build",
    "build_linux_x64": "npx electron-packager . --platform=linux --arch=x64 --ignore=\"^(/build|/scripts)\" --out build",
    "build_linux_arm64": "npx electron-packager . --platform=linux --arch=arm64 --ignore=\"^(/build|/scripts)\" --out build",
    "build_darwin_x64": "npx electron-packager . --platform=darwin --arch=x64 --ignore=\"^(/build|/scripts)\" --out build",
    "build_darwin_arm64": "npx electron-packager . --platform=darwin --arch=arm64 --ignore=\"^(/build|/scripts)\" --out build --overwrite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@electron/packager": "^19.0.0",
    "electron": "^38.4.0"
  },
  "dependencies": {
    "command-line-args": "^5.2.1",
    "webrtc-adapter": "^9.0.3",
    "ws": "^8.12.1"
  }
}
